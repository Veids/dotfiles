{
    "C_Cpp.clang_format_style": "Google",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "c++17",
    "carbon.useBrowserCache": true,
    "carbon.windowControls": false,
    "cmake.configureOnOpen": true,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "editor.enableFontLigatures": true,
    "editor.fontFamily": "'Hack Nerd Font Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "glsllint.glslangValidatorPath": "/usr/bin/glslangValidator",
    "vscode-neovim.neovimInitPath": "~/.config/nvim/init.vim",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "workbench.colorTheme": "Atom One Dark",
    "window.zoomLevel": 0,
    "editor.lineNumbers": "relative",
    "editor.tabSize": 2
}
